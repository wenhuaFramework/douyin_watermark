.qq_group {
    display: flex;
    justify-content: center;
    align-self: center;
    font-size: 28rpx;
    color: #ed3f14;
    height: 80rpx;
    width: 500rpx;
    border: #dbd9d9 1px solid;
    border-radius: 50rpx;
    line-height: 80rpx;
}

.qq_group image {
    height: 50rpx;
    width: 55rpx;
    padding: 15rpx;
}

.tuijian {
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
    align-self: center;
    width: 620rpx;
    margin: 40rpx;
}

.tuijian .itme {
    display: inline-flex;
    align-items: center;
    width: 280rpx;
    height: 60rpx;
    background-color: white;
    box-shadow: 4rpx 4rpx 5rpx #cecdcd;
    margin: 6rpx 5rpx;
    padding: 0 10rpx;
}

.tuijian .itme image {
    width: 30rpx;
    height: 30rpx;
    line-height: 30rpx;
}

.tuijian text {
    font-size: 32rpx;
    width: 250rpx;
    color: #f90;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.zhifubao {
    display: flex;
    flex-direction: column;
    align-items: center;
    margin-top: 0rpx;
    margin-bottom: 0rpx;
}

.zhifubao .wenzi {
    display: inline-flex;
    justify-content: center;
    font-size: 30rpx;
    width: 750rpx;
    margin-top: 10rpx;
}

.zhifubao .left {
    color: #ed3f14;
}

.zhifubao .fuzhi {
    margin-left: 15rpx;
    color: #00adf9;
}

.zhifubao image {
    margin-top: 0rpx;
    width: 100%;
    height: 1000rpx;
}

.banners {
    border-top: 0rpx solid #D9D9D9;
    overflow: hidden;
    margin-top: 10px;
}

.banner {
    position: relative;
    float: left;
    padding: 5px 0px;
    width: 50%;
    height: 170rpx;
    box-sizing: border-box;
    border-right: 0rpx solid #D9D9D9;
    border-bottom: 0rpx solid #D9D9D9;
}

.bf_input_down {
    margin-left: 45rpx;
    margin-bottom: 10rpx;
    margin-right: 45px;
    display: block;
    width: 100%;
    height: 166rpx;
    background: url(https://wx.muzzz.cn/addons/qsy_un66188/images/fx.png) no-repeat;
    background-size: 100% 100%;
    margin: 10px auto;
    line-height: 36px;
    text-align: center;
    font-size: 18px;
}

.bf_input_down::after {
    border: none;
}

.jiaocheng {
    margin-left: 45rpx;
    margin-bottom: 10rpx;
