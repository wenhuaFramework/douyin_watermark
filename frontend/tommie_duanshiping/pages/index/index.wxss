    background-color: rgb(0,0,0);
    border-top-left-radius: 50rpx;
    border-bottom-left-radius: 50rpx;
}

.share-img-button {
    line-height: 80rpx;
    height: 80rpx;
    width: 220rpx;
    border-top-left-radius: 50rpx;
    border-bottom-left-radius: 50rpx;
    border: none;
    background: none;
    color: white;
    font-size: 32rpx;
}

.share-img-button::after {
    border: none;
    background: none;
}

.ad {
