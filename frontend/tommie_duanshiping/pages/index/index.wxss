page {
    background-color: #FFFFFF;
}

.container {
    display: flex;
    flex-direction: column;
    padding: 0 0 30rpx 0;
}

.gonggao {
    width: 750rpx;
    position: fixed;
    top: 0;
}

.head_img {
    width: 100%;
    height: auto;
    position: relative;
    padding-bottom: 2rpx;
    background: #eee;
}

.head_imgs {
    width: 100vw;
    height: auto;
}

.head_close {
    width: 64rpx;
    height: 64rpx;
    position: absolute;
    top: 14rpx;
    right: 30rpx;
}

.head_closeimg {
    width: 36rpx;
    height: 36rpx;
    margin: 14rpx;
}

.hide {
    display: none;
}

.show {
    display: block;
}

.text_color {
    color: #555;
}

.top {
    position: relative;
    text-align: center;
    margin-bottom: 20rpx;
    margin-top: 30rpx;
}

.top .help {
    position: absolute;
    right: 20rpx;
    bottom: 1rpx;
    height: 60rpx;
    width: 60rpx;
}

.inputmain {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 750rpx;
    margin: 10rpx 0;
    border: none;
}

.jiexibut {
    display: inline-flex;
    justify-content: center;
    align-items: center;
    width: 145rpx;
    height: 86rpx;
    background: linear-gradient(#ffd800,#ffbb00);
    color: #563f00;
    font-size: 36rpx;
    border-top-right-radius: 30rpx;
    border-bottom-right-radius: 30rpx;
    border: none;
}

.login-button {
    width: 145rpx;
    border: none;
    background: none;
    color: #563f00;
    font-size: 36rpx;
    border: none;
}

.login-button::after {
    border: none;
    background: none;
}

.button {
    display: flex;
    justify-content: center;
}

.videos {
    display: flex;
